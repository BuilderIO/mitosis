{
  "name": "@builder.io/mitosis-cli",
  "version": "0.0.52",
  "description": "mitosis CLI",
  "types": "build/types/types.d.ts",
  "bin": {
    "mitosis": "bin/mitosis"
  },
  "scripts": {
    "format": "prettier --loglevel warn --write \"**/*.{js,ts,tsx,json}\"",
    "clean-build": "tsc --build --clean tsconfig.build.json && rm -rf ./dist",
    "compile": "tsc --build --verbose --pretty tsconfig.build.json",
    "start": "tsc --watch",
    "build": "yarn format && yarn clean-build && yarn compile",
    "prepublishOnly": "yarn build",
    "remove-stableVersion": "./scripts/remove-stableVersion.sh",
    "release:dev": "yarn build && yarn run remove-stableVersion && yarn version prerelease && yarn npm publish --tag dev",
    "release:patch": "yarn build && yarn run remove-stableVersion && yarn version patch && yarn npm publish",
    "test": "vitest run",
    "test:watch": "vitest",
    "snapupdate": "vitest --update",
    "coverage": "vitest run --coverage"
  },
  "files": [
    "dist",
    "bin"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.17.8",
    "@builder.io/mitosis": "workspace:*",
    "@vue/compiler-sfc": "^3.1.5",
    "babel-preset-solid": "^1.3.13",
    "chalk": "^4.1.0",
    "debug": "^4.3.4",
    "dedent": "^0.7.0",
    "esbuild": "^0.12.16",
    "fast-glob": "^3.2.11",
    "fp-ts": "^2.12.2",
    "fs-extra": "^9.1.0",
    "globby": "^11.0.2",
    "gluegun": "latest",
    "html-minifier": "^4.0.0",
    "json5": "^2.2.1",
    "lodash": "^4.17.21",
    "micromatch": "^4.0.4",
    "parse5": "^3.0.3",
    "prettier": "^2.6.1",
    "vue-template-es2015-compiler": "^1.9.1",
    "vue-template-validator": "^1.1.5"
  },
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.180",
    "@types/micromatch": "^4.0.2",
    "@types/node": "^12.7.11",
    "ts-node": "^8.4.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vitest": "^0.24.5"
  }
}
