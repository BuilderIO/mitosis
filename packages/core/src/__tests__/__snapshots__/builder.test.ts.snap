          css={{
            display: \\"block\\",
          }}
        />
        <textarea
          placeholder=\\"How was your experience?\\"
          css={{
            display: \\"block\\",
          }}
        />
        <button
          onClick={(event) => {
            event.preventDefault();
            state.showReviewPrompt = false;
          }}
          css={{
            display: \\"block\\",
          }}
        >
          Submit
        </button>
      </Show>
      <For each={state.reviews}>
        {(review, index) => (
          <div
            $name=\\"Review\\"
            key={review.id}
            css={{
              margin: \\"10px\\",
              padding: \\"10px\\",
