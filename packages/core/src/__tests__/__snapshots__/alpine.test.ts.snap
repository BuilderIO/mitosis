            : {}),
        };
      },

      get shouldWrap() {
        return !this.component?.noWrap;
      },

      get renderComponentProps() {
        return {
          blockChildren: this.useChildren,
          componentRef: this.component?.component,
          componentOptions: {
            ...getBlockComponentOptions(this.useBlock),

            /**
