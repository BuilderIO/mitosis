    "@types/express": "4.17.13",
    "@types/node": "^16",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "autoprefixer": "10.4.7",
    "eslint": "8.16.0",
    "eslint-plugin-qwik": "0.0.34",
    "express": "4.17.3",
    "node-fetch": "2.6.7",
    "postcss": "8.4.14",
